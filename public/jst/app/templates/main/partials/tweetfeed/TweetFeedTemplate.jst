<div class="col-sm-4 tweetfeed <% if (active) { print('active') } %>" data-id="<%= screen_name %>">
  <div class="title">
    <h3><a href="<%= user_url %>" class="link-color" target="_blank">@<%= screen_name %></a> <span class="small hidden icon-spinner animate-spin"></span></h3>
  </div>
  <div class="list-tweet"></div>
</div>